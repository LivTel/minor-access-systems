		       <xbin> 2 </xbin>
		       <ybin> 2 </ybin>
		     </detector>
		  </inst-common>
		  <ccd-config>
		     <filter> SDSS-U <filter>
		     <lower-slide> clear  </lower-slide>
                     <upper-slide> ND3 </upper-slide>
		  </ccd-config>
	       </inst-config>
            </value>
         </param>

   </request>
</p2ml>

