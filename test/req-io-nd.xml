<p2ml>
   <request>
      <action> add-inst-config </action>
     
         <param name = "proposal-id">
             <value> LT_Phase2_001/NSO/XNSO/NSO_Priority_1 </value>]
         </param>
	 
	 <param name = "instrument-config">
